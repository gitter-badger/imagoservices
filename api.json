{
    "swagger": "2.0",
    "info": {
        "version": "v1",
        "title": "UAV Management Api",
        "description": "A REST Api to manage uavs and related data."
    },
    "host": "localhost",
    "schemes": ["http", "https"],
    "basePath": "/",
    "paths": {
        "/device": {
            "get": {
                "tags": ["Devices"],
                "operationId": "device_get",
                "consumes": [],
                "produces": ["application/json",
                    "text/json"
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Device"
                            }
                        }
                    }
                },
                "deprecated": false
            }
        },
        "/zonealert": {
            "get": {
                "tags": ["Zonealert"],
                "operationId": "zonealerts_get",
                "consumes": [],
                "produces": ["application/json",
                    "text/json"
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Zonealert"
                            }
                        }
                    }
                },
                "deprecated": false
            }
        },        
        "/uav": {
            "get": {
                "tags": ["Uavs"],
                "operationId": "uav_get",
                "consumes": [],
                "produces": ["application/json",
                    "text/json"
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Uav"
                            }
                        }
                    }
                },
                "deprecated": false
            }
        },
        "/uav/{latitude}/{longitude}/{radius}": {
            "get": {
                "tags": ["Uav"],
                "operationId": "uav_getByPosition",
                "consumes": [],
                "produces": ["application/json",
                    "text/json"
                ],
                "parameters": [{
                    "name": "latitude",
                    "in": "path",
                    "required": true,
                    "type": "number",
                    "format": "double"
                }, {
                    "name": "longitude",
                    "in": "path",
                    "required": true,
                    "type": "number",
                    "format": "double"
                }, {
                    "name": "radius",
                    "in": "path",
                    "required": true,
                    "type": "integer",
                    "format": "int32"
                }],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Uav"
                            }
                        }
                    }
                },
                "deprecated": false
            }
        }
    },
    "definitions": {
        "Uav": {
            "type": "object"
        },
        "Device": {
            "type": "object"
        },
        "Zonealert": {
            "type": "object"
        },
         "Zone": {
            "type": "object"
        }
   }
}